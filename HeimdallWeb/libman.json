{
  "version": "3.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "bootstrap@5.3.2",
      "destination": "wwwroot/lib/bootstrap"
    },
    {
      "provider": "unpkg",
      "library": "sweetalert2@11",
      "destination": "wwwroot/lib/sweetalert2"
    },
    {
      "provider": "unpkg",
      "library": "axios@1",
      "destination": "wwwroot/lib/axios"
    },
    {
      "provider": "unpkg",
      "library": "@fortawesome/fontawesome-free@6.5.0",
      "destination": "wwwroot/lib/fontawesome"
    },
    {
      "provider": "unpkg",
      "library": "bootstrap-icons@1.13.1",
      "destination": "wwwroot/lib/bootstrap-icons"
    }
  ]
}